{
{% if addressLine1 is defined and addressLine1 is not none and addressLine1 != omit %}
  "addressLine1": "{{ addressLine1 }}",
{% endif %}
{% if addressLine2 is defined and addressLine2 is not none and addressLine2 != omit %}
  "addressLine2": {{ addressLine2 }},
{% endif %}
{% if city is defined and city is not none and city != omit %}
  "city": ["{{ city }}"],
{% endif %}
  "confirmLocalPasswd": "{{ confirmLocalPasswd }}",
{% if company is defined and company is not none and company != omit %}
  "company": {{ company }},
{% endif %}
  "email": "{{ email }}",
  "fullName": "{{ fullName }}",
  "localPasswd": "{{ localPasswd }}",
{% if newUser is defined and newUser is not none and newUser != omit %}
  "newUser": {{ newUser }},
{% endif %}
{% if phoneNumber is defined and phoneNumber is not none and phoneNumber != omit %}
  "phoneNumber": {{ phoneNumber }},
{% endif %}
{% if postalCode is defined and postalCode is not none and postalCode != omit %}
  "postalCode": {{ postalCode }},
{% endif %}
{% if publicSshKey is defined and publicSshKey is not none and publicSshKey != omit %}
  "publicSshKey": {{ publicSshKey }},
{% endif %}
  "role": {{ role }},
{% if stateOrProvince is defined and stateOrProvince is not none and stateOrProvince != omit %}
  "stateOrProvince": {{ stateOrProvince }},
{% endif %}
{% if tenantId is defined and tenantId is not none and tenantId != omit %}
  "tenantId": {{ tenantId | int }},
{% endif %}
  "username": "{{ username }}"
}
